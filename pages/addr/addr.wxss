/* pages/login/addAddr/addAddr.wxss */
page {
  height: 100%;
}
.parent {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center
}
.v-img {
  display: flex;
  justify-content: center;
  height: 10%;
  width: 100%;
  align-items: center;
  
}
.v-data {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 40%;
  width: 100%;
  align-items: center;
  
}
.v-data>view {
  display: flex;
  align-items: center;
  height: 17%;
  width: 80%;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  border-radius: 10rpx;
  border: 0.5rpx solid #bababa;
  justify-content: space-around
}
.v-btn {
  height: 10%;
  margin-top: 70rpx;
  
  width: 80%;
}
.txt-size {
  font-size: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
button {
  width: 100%;
  height: 70%;
  border-radius: 85rpx;
  background-color: #89d40c
}
